{
    "name": "encore-labs/guzzle-bundle-header-forward-plugin",
    "type": "library",
    "description": "Guzzle plugin that allow you to forward headers from your current request into the the guzzle one automatically. Forked to support Symfony 4",
    "keywords": [
        "middleware",
        "plugin",
        "framework",
        "http",
        "rest",
        "web service",
        "curl",
        "client",
        "HTTP client",
        "guzzle",
        "header",
        "forward"
    ],
    "homepage": "https://github.com/EncoreLabs/GuzzleBundleHeaderForwardPlugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Roches",
            "email": "adrien@roc-it.tech",
            "homepage": "https://github.com/Neirda24"
        },
        {
            "name": "Tom Cawthorn"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/Neirda24/GuzzleBundleHeaderForwardPlugin/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.0",
        "eightpoints/guzzle-bundle": "~8.0",
        "symfony/http-kernel": "~5.0",
        "symfony/config": "~5.0",
        "symfony/dependency-injection": "~5.0",
        "symfony/expression-language": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.1"
    },
    "autoload": {
        "psr-4": {
            "EncoreLabs\\Bundle\\GuzzleBundleHeaderForwardPlugin\\": "src"
        }
    }
}
